    Sub Window_OnLoad()
    window.resizeTo 300,200
    window.moveTo (screen.width/2)-1366/2,360/2
    'MsgBox document.body.offsetHeight
    End Sub

    Function yesBtn()
    CreateObject("WScript.Shell").Run "calce"
    End Function

    Function noBtn()
        MsgBox "Close"
        window.close
    End Function

  </script>
